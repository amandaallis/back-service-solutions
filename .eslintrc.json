{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parser": "babel-eslint",
    "extends": [
        "airbnb-base", 
        "prettier"],
    
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-use-before-define": "off",
        "semi": ["error", "always"]
    }
}
